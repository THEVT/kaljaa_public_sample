<!DOCTYPE html>
<html>
<head>
  <title><%=@title%></title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
	<%= yield(:head) %>
</head>
<body>
	<div id="main">
		<%= render "layouts/navigation" %>
		<div id="wrapper" class="clearfix">
			<%= yield %>
		</div>
		<%= debug(params) if Rails.env.development? %>
	</div>
</body>
</html>
