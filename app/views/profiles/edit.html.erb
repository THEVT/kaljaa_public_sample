<h1>Update your Settings</h1>

<%= form_for(@profile) do |f| %>
	<%= render 'shared/error_messages', object: f.object %>

	<%= f.label :s1 %>
	<%= f.text_field :s1 %>

	<%= f.label :s2 %>
	<%= f.text_field :s2 %>

	<%= f.label :s3 %>
	<%= f.text_field :s3 %>

	<%= f.label :s4 %>
	<%= f.text_field :s4 %>

	<%= f.label :s5 %>
	<%= f.text_field :s5 %>

	<%= f.label :s6 %>
	<%= f.text_field :s6 %>
	
	<%= f.submit "Save changes"%>
<% end %>
