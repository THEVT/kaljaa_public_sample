
<li>
	<%= link_to article.title, article %> <br>
	<%= article.profile.s1 %>
	Posted <%= time_ago_in_words(article.created_at) %> ago.
	<% if current_profile?(article.profile) %>
		<%= link_to 'Edit Article',  edit_article_path(article.id) %>
		<%= link_to "delete", article, method: :delete,
							 data: { confirm: "You sure?" },
							 title: article.content %>
	<% end %>

</li>
